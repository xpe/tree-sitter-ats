{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ats",
      "camelcase": "Ats",
      "title": "ATS",
      "scope": "source.ats",
      "file-types": [
        "ats"
      ],
      "injection-regex": "^ats$",
      "class-name": "TreeSitterAts"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "none",
    "description": "ATS parser",
    "authors": [
      {
        "name": "David James",
        "email": "davidcjames@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-ats"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}